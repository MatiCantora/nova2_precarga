.overlay_alphacube {
	background-color: #85BBEF;
	filter:alpha(opacity=60);
	-moz-opacity: 0.6;
	opacity: 0.6;
   
}




.alphacube_n {
  /*background: transparent url(alphacube/top-middle.gif) repeat-x 0 0;	*/
  background: linear-gradient(white 50%,  #F6F6F9 50%);
  border-top: 1px solid #B7B7B9;
  height:25px;
}


.alphacube_w {
  /*background: transparent url(alphacube/frame-left.gif) repeat-y top left;	*/	
  background: #F6F6F9;
  	
  border-left: 1px solid #B7B7B9;
  width:7px;
}

.alphacube_e {
  /*background: transparent url(alphacube/frame-right.gif) repeat-y top right;		*/
  background: #F6F6F9;

  border-right: 1px solid #B7B7B9;	
  width:7px;	  
}



.alphacube_s {
  /*background: transparent url(alphacube/bottom-middle.gif) repeat-x 0 0;*/	
  background: #F6F6F9;		
  		
  border-bottom: 1px solid #B7B7B9;
  height:7px;
}



.alphacube_sizer {
	cursor:se-resize;	
}

@media screen and (max-width: 799px) {
    .alphacube_close {
	    width: 24px;
	    height: 24px;
	    background: transparent url(alphacube/button-back-focus.png) no-repeat 0 0;		
        background-size: 20px 20px;	
	    position: absolute;
	    top:4px;
	    left: 5px;
	    cursor:pointer;
	    z-index:1000;
    }

    .alphacube_close_back {
	    position: absolute;
        font-size: 14px;
	    left: 18px;
        top: 2px;
	    cursor:pointer;
	    z-index:1000;
    }


    .alphacube_nw {
      /*background: transparent url(alphacube/left-top.gif) no-repeat 0 0;	*/	
      background: linear-gradient(white 50%,  #F6F6F9 50%);	

      border-top: 1px solid #B7B7B9;
      border-left: 1px solid #B7B7B9;

      width:10px;
      height:25px;
    }

    .alphacube_sw {
      /*background: transparent url(alphacube/bottom-left-c.gif) no-repeat 0 0;		*/	
      background: #F6F6F9;		

      border-bottom: 1px solid #B7B7B9;
      border-left: 1px solid #B7B7B9;

      width:7px;
      height:7px;
    }
    
    .alphacube_ne {
      /*background: transparent url(alphacube/right-top.gif) no-repeat 0 0;		*/	
      background: linear-gradient(white 50%,  #F6F6F9 50%);	

      border-top: 1px solid #B7B7B9;
      border-right: 1px solid #B7B7B9;

      width:10px;
      height:25px;
    }

    .alphacube_se, .alphacube_sizer  {
      /*background: transparent url(alphacube/bottom-right-c.gif) no-repeat 0 0;		*/	
      background: #F6F6F9;		

      border-bottom: 1px solid #B7B7B9;
      border-right: 1px solid #B7B7B9;

      width:7px;
      height:7px;
    }
}

@media screen and (min-width: 800px) {
    .alphacube_close {
        width: 23px;
        height: 23px;
        background: transparent url(alphacube/button-close-focus.gif) no-repeat 0 0;
        position: absolute;
        top: 0px;
        right: 11px;
        cursor: pointer;
        z-index: 1000;
    }

    .alphacube_close_back {
	    display: none
    }


    .alphacube_nw {
      /*background: transparent url(alphacube/left-top.gif) no-repeat 0 0;	*/	
      background: linear-gradient(white 50%,  #F6F6F9 50%);	

      border-top-left-radius: 10px;
      border-top: 1px solid #B7B7B9;
      border-left: 1px solid #B7B7B9;

      width:10px;
      height:25px;
    }

    .alphacube_se, .alphacube_sizer  {
      /*background: transparent url(alphacube/bottom-right-c.gif) no-repeat 0 0;		*/	
      background: #F6F6F9;		

      border-bottom-right-radius: 7px;
      border-bottom: 1px solid #B7B7B9;
      border-right: 1px solid #B7B7B9;

      width:7px;
      height:7px;
    }

    .alphacube_sw {
      /*background: transparent url(alphacube/bottom-left-c.gif) no-repeat 0 0;		*/	
      background: #F6F6F9;		

      border-bottom-left-radius: 7px;
      border-bottom: 1px solid #B7B7B9;
      border-left: 1px solid #B7B7B9;

      width:7px;
      height:7px;
    }


    .alphacube_ne {
      /*background: transparent url(alphacube/right-top.gif) no-repeat 0 0;		*/	
      background: linear-gradient(white 50%,  #F6F6F9 50%);	

      border-top-right-radius: 10px;
      border-top: 1px solid #B7B7B9;
      border-right: 1px solid #B7B7B9;

      width:10px;
      height:25px;
    }
}


.alphacube_minimize {
	width: 23px;
	height: 23px;
	background: transparent url(alphacube/button-min-focus.gif) no-repeat 0 0;			
	position:absolute;
	top:0px;
	right:55px;
	cursor:pointer;
	z-index:1000;
}

.alphacube_maximize {
	width: 23px;
	height: 23px;
	background: transparent url(alphacube/button-max-focus.gif) no-repeat 0 0;			
	position:absolute;
	top:0px;
	right:33px;
	cursor:pointer;
	z-index:1000;
}

.alphacube_title {
	float:left;
	height:14px;
	font-size:14px;
	text-align:center;
	margin-top:2px;
	width:100%;
	color:#123456;
}

.alphacube_content {
	overflow:auto;
	color: #000;
	/*font-family: Tahoma, Arial, sans-serif;
    font: 12px arial;*/
	background:#FDFDFD;
}

/* For alert/confirm dialog */
.alphacube_window {
	border:1px solid #F00;	
	background: #FFF;
	padding:20px;
	margin-left:auto;
	margin-right:auto;
	width:400px;
}

.alphacube_message {
  font: 13px arial /*12px arial*/;
	text-align:center;
	width:100%;
	padding-bottom:10px;
}

.alphacube_buttons {
    font: 13px arial; /*No estaba*/
	text-align:center;
	width:100%;
}

.alphacube_buttons input {
	width:20%;
	margin:10px;
    min-width:80px;
}

.alphacube_progress {
	float:left;
	margin:auto;
	text-align:center;
	width:100%;
	height:16px;
	background: #FFF url('alert/progress.gif') no-repeat center center
}

.alphacube_wired_frame {
	background: #FFF;
	filter:alpha(opacity=60);
	-moz-opacity: 0.6;
	opacity: 0.6;	
}


