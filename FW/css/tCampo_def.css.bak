.campo_def_div_contenedor 
            {
            -moz-border-radius: 0.33em;
            -webkit-border-radius: 0.33em;
            border-radius: 0.33em;
            background-color: white; 
            z-index: 500; 
            height: 154px; 
            border: solid #d0d0d0 1px;
            } 

.campo_def_cbmulti_select 
          {
          background-color: #b7b7b7 !important;
          color: black !important;
          }


.campo_def_cb_item 
         {
         }

.campo_def_cb_item:hover 
         {
         cursor:pointer !important
         }

.campo_def_cb_item_disable {
         color: gray !important
}

    .campo_def_cb_item_disable:hover {
        cursor: not-allowed !important
         }

.img_find {
    border: none;
    width: 16px;
    cursor: pointer;
    background-image: url(/FW/image/campo_def/down.png);
    background-repeat: no-repeat;
    background-position: left center;
}


.img_calendar {
    border: none;
    width: 16px;
    cursor: pointer;
    background-image: url(/FW/image/campo_def/calendar.png);
    background-repeat: no-repeat;
    background-position: left center;
}


.img_preview {
    border: none;
    width: 16px;
    cursor: pointer;
    background-image: url(/FW/image/campo_def/preview.png);
    background-repeat: no-repeat;
    background-position: left center;
}


.img_down {
    float: right;
    position: absolute;
    z-index: 3;
    right: 2px;
    top: 2px;
}

    .img_clear {
    border: none;
    width: 100%;
    height:100%;
    cursor: pointer;
    background-image: url(/FW/image/campo_def/cancelar.png);
    background-repeat: no-repeat;
    background-position: center center;
    padding:0px 8px;
}
